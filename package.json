    "@types/node": "^18.11.18",
    "typescript": "^4.9.4",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lerna": "^6.4.1",
    "prettier": "^2.8.3"
